    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.15.0",
    "express": "^5.0.0-alpha.2",
    "express-cluster-stability": "^1.2.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.1.2"
